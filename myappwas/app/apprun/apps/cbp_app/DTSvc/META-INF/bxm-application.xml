<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bxm-application xmlns="http://www.es-code.com/schema/application" xmlns:ns2="http://www.es-code.com/schema/common" type="online" reference-applications="AC,AR,AT,CM,CORE-intrfc,PD,SM,ST,SV,DC" shared="true" loglevel="Default" name="DTSvc">
    <description>DTSvc application is the default config.</description>
    <datasource>
        <jndi-datasource name="CBB" jndi-datasource="CBBNXA" jndi-xadatasource="CBBXA"/>
    </datasource>
    <datasource>
        <jdbc-datasource name="CBB" driver-classname="${driver-classname}" uri="${uri}" username="${username}" enc-password="${password}" maxActive="10" maxIdle="10" validationQuery="select 1 from dual" validationQueryTimeout="60"/>
    </datasource>
    <preprocessor-bean order="1" bean="bankware.corebanking.servicemanagement.executor.business.PreServiceHandlerImpl"/>
    <postprocessor-bean order="1" bean="bankware.corebanking.servicemanagement.executor.business.PostServiceHandlerImpl"/>
    <preloading-classes>
        <include name="bankware.corebanking.servicemanagement.executor.business.PreServiceHandlerImpl"/>
    </preloading-classes>
</bxm-application>
